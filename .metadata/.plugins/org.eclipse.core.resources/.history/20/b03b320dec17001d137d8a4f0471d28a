package org.Programs;

public class Armstrong {
	
	public static void main(String  [] args){
		int count = 0;
		for(int i =1; i<=1000; i++){
			
			
			int n = i;
			int j=0;
			while(n>0){
								
					int a = n%10; 
					j= j +(a*a*a); 
					n  =n/10;    
			}
			
			if (j==i){
				System.out.println("Armstrong number is:" +i);
				count++;
			
			}
			}
			
			System.out.println(count);
		}
}
	


			
			
			
