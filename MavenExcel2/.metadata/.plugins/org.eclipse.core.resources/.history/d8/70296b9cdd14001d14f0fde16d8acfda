		WebElement com18 =  driver.findElement(By.xpath("//a[contains(text(),'Prime Infosoft')]"));
		List<String> name = new ArrayList();
		
		
		
		name.add(job1.getText());
		name.add("Venkatesh");
		name.add("Dhanvika");
		name.add("Nivetha");
		name.add("Deepan");
		
			for (int i=0; i<name.size(); i++){
				
				HSSFRow r = sheet.createRow(i+1);
				HSSFCell c = r.createCell(0);
				c.setCellValue(name.get(i));
				
			}	
		work.write(f);
		work.close();	
		
