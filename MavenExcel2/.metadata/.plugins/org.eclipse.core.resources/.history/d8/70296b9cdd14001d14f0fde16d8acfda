		WebElement com10 =  driver.findElement(By.xpath("//a[contains(text(),'workXforce Smart technologies')]"));
		WebElement job11 =  driver.findElement(By.xpath("//a[contains(text(),'Automation Tester - Selenium')]"));
		WebElement com11 =  driver.findElement(By.xpath("//a[contains(text(),'People Konnect')]"));
		WebElement job12 =  driver.findElement(By.xpath("//a[contains(text(),'Selenium Test Engineer - Manual/Automation Testing')]"));
		WebElement com12 =  driver.findElement(By.xpath("//a[contains(text(),'cliqhr.com')]"));
		WebElement job13=  driver.findElement(By.xpath("//a[contains(text(),'Quality Automation Tester - Java/Selenium')]"));
		WebElement com13 =  driver.findElement(By.xpath("//a[contains(text(),'Senior Automation Test Engineer (Contingency)')]"));
		WebElement job14 =  driver.findElement(By.xpath("//a[contains(text(),'Equiniti ICS')][1]"));
		WebElement com14 =  driver.findElement(By.xpath("//a[contains(text(),'Automation Test Engineer')]"));
		WebElement job15 =  driver.findElement(By.xpath("//a[contains(text(),'Equiniti ICS')][2]"));
		WebElement com15 =  driver.findElement(By.xpath("//a[contains(text(),'Senior Automation Test Engineer (Contingency)')]"));
		WebElement job16 =  driver.findElement(By.xpath("//a[contains(text(),'Equiniti ICS')][3]')]"));
		WebElement com16 =  driver.findElement(By.xpath("//a[contains(text(),'Automation Test Engineer')]"));
		WebElement job17 =  driver.findElement(By.xpath("//a[contains(text(),'Equiniti ICS')][4]')]"));
